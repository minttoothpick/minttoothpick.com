<article class="Grid Article space-items-lg">
  <header class="">{# TODO: no bottom margins #}
    {% if item.data.display_title != false %}
      <h1 class="">{{ item.data.title | safe }}</h1>{# mt-xxs #}
    {% endif %}
    <time class="text-muted">{{ item.data.dateCreated | readableDate }}</time>
  </header>
  {{ item.templateContent | safe }}
</article>
